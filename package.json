  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gurisa/arjuna/issues"
  },
  "homepage": "https://github.com/gurisa/arjuna#readme",
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {
    "@slack/webhook": "^5.0.3",
    "amqplib": "^0.6.0",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
